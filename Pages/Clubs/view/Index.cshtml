@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}
<h2>Club Representative's Clubs</h2>




<table class="table">
    <thead>
        <th>Name</th>
        <th>id</th>
        <th>Location</th>
        <th></th>
        <th></th>
    </thead>
    <tbody>
        @foreach (var Club in Model.Clubss)
        {
            <tr>
                <td>@Club.Club_name</td>
                <td>@Club.id</td>
                <td>@Club.Locations</td>
                <td><a href="/Club/Edit?id=@Club.id">Edit</a></td>
                <td><a href="/Club/Delete?id=@Club.id">Delete</a></td>
            </tr>
        }
    </tbody>
</table>

