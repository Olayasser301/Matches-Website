@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}
<h2>System admin</h2>

<a href="/Clubs/New">New Club</a>
<a href ="/Stadiums/New">New Staduim</a>
<a href = "/Clubs/Delete">Delete Club</a>
<a href ="/Stadiums/Delete">Delete Staduim</a>
